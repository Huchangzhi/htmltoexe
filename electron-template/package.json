{
  "name": "main",
  "version": "2.0.2",
  "description": "由网页打包而来的软件罢了",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [
    "electron"
  ],
  "author": "Unknownhcz",
  "license": "MIT",
  "devDependencies": {
    "electron": "^22.3.27",
    "electron-builder": "^22.14.13"
  },
  "dependencies": {
    "electron-config": "^2.0.0"
  },
  "build": {
    "appId": "com.htmltoexe.main",
    "productName": "main",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "icon.png",
      "publish": null
    },
    "portable": {
      "artifactName": "${productName}.${ext}"
      }
  }
}